<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.springboot.chat.mapper.TChatLogMapper">
    <select id="findOne" resultType="com.springboot.chat.entity.TChatLog">
		select
			*
		from T_CHAT_LOG
		where CHAT_GROUP_ID = #{chat_group_id}
	</select>
	<insert id="insert">
		insert into T_CHAT_LOG (CHAT_GROUP_ID, CHAT_DATE, USER_ID, COMMENT)
		values(
			 #{form.chat_group_id}
			,#{form.chat_date}
			,#{form.user_id}
			,#{form.comment}
		)
	</insert>
</mapper>